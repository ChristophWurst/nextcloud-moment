{
  "name": "@nextcloud/moment",
  "version": "1.1.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --hide-modules --config webpack.prod.js && tsc --emitDeclarationOnly",
    "build:doc": "typedoc --ignoreCompilerErrors --excludeNotExported --excludeExternals --out dist/doc lib && touch dist/doc/.nojekyll",
    "check-types": "tsc",
    "dev": "NODE_ENV=development webpack --progress --watch --config webpack.dev.js",
    "l10n:extract": "node build/extract-l10n.js",
    "test": "mochapack --webpack-config webpack.test.js --require test/setup.js \"test/**/*.test.js\"",
    "test:watch": "mochapack -w --webpack-config webpack.test.js --require test/setup.js \"test/**/*.test.js\""
  },
  "keywords": [
    "nextcloud"
  ],
  "homepage": "https://github.com/nextcloud/nextcloud-moment#readme",
  "author": "Christoph Wurst",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/nextcloud/nextcloud-moment"
  },
  "dependencies": {
    "@nextcloud/l10n": "1.1.0",
    "core-js": "3.6.4",
    "jed": "^1.1.1",
    "moment": "2.24.0",
    "node-gettext": "^2.0.0"
  },
  "devDependencies": {
    "@atomic-reactor/webpack-po-loader": "0.0.3",
    "@babel/cli": "7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "7.8.3",
    "@nextcloud/browserslist-config": "^1.0.0",
    "@types/node-gettext": "^2.0.1",
    "babel-jest": "25.1.0",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "gettext-extractor": "3.5.2",
    "gettext-parser": "^4.0.2",
    "jsdom": "^16.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^7.0.1",
    "mochapack": "^1.1.13",
    "ts-loader": "^6.2.1",
    "typedoc": "^0.16.7",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": [
    "extends @nextcloud/browserslist-config"
  ]
}
