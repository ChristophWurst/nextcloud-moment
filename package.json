{
  "name": "@nextcloud/moment",
  "version": "1.2.1",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --node-env production --progress --config webpack.prod.js && tsc --emitDeclarationOnly",
    "build:doc": "typedoc --out dist/doc lib && touch dist/doc/.nojekyll",
    "check-types": "tsc",
    "dev": "webpack --node-env development --progress --watch --config webpack.dev.js",
    "l10n:extract": "node build/extract-l10n.js",
    "test": "mochapack --webpack-config webpack.test.js --require test/setup.js \"test/**/*.test.js\"",
    "test:watch": "mochapack -w --webpack-config webpack.test.js --require test/setup.js \"test/**/*.test.js\""
  },
  "keywords": [
    "nextcloud"
  ],
  "homepage": "https://github.com/nextcloud/nextcloud-moment#readme",
  "author": "Christoph Wurst",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/nextcloud/nextcloud-moment"
  },
  "dependencies": {
    "@nextcloud/l10n": "^1.4.1",
    "core-js": "^3.21.1",
    "jed": "^1.1.1",
    "moment": "^2.29.2",
    "node-gettext": "^3.0.0"
  },
  "devDependencies": {
    "@atomic-reactor/webpack-po-loader": "^0.0.3",
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@nextcloud/browserslist-config": "^2.2.0",
    "@types/node-gettext": "^3.0.3",
    "babel-jest": "^28.1.1",
    "babel-loader": "^8.2.4",
    "chai": "^4.3.6",
    "gettext-extractor": "^3.5.4",
    "gettext-parser": "^5.1.2",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^10.2.0",
    "mochapack": "^2.1.4",
    "ts-loader": "^9.2.8",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "browserslist": [
    "extends @nextcloud/browserslist-config"
  ],
  "engines": {
    "node": "^16.0.0",
    "npm": "^7.0.0 || ^8.0.0"
  }
}
